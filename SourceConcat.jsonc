{
    "root.paths": [
        "."
    ],
    "output.path": "~/Documents/context.md",
    "output.mode": "clipboard-file",
    "include.files": [
        "*.jl",
        "*.md",
        "*Project.toml",
        "*SourceConcat.json"
    ],
    "exclude.files": [
        "_DEPRECATED/**",
        ".git/**",
        "dev/**",
        "*.jls",
        "*.jsonl"
    ],
    "print.head.lines": 60,
    "use.gtignore": true
}